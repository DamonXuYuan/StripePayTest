{
  "name": "next-fronted",
  "version": "1.0.0",
  "private": true,
  "sideEffects": [
    "*.css",
    "*.less",
    "*.scss"
  ],
  "scripts": {
    "postinstall": "npm run gen:theme-typings",
    "clean": "rimraf .next out",
    "dev": "next dev",
    "build": "npm run clean && cross-env EVN_FILE_NAME=dev next build",
    "build:prod": "npm run clean && cross-env EVN_FILE_NAME=prod next build",
    "export": "cross-env EVN_FILE_NAME=dev next export",
    "export:prod": "cross-env EVN_FILE_NAME=prod next export",
    "start": "cross-env EVN_FILE_NAME=dev next start -p ${PORT:=3000}",
    "start:prod": "cross-env EVN_FILE_NAME=prod next start -p ${PORT:=3004}",
    "lint": "npm run tslint && npm run eslint && npm run stylelint",
    "eslint": "eslint --fix src/",
    "tslint": "tsc --project tsconfig.json --pretty --noEmit",
    "stylelint": "stylelint \"src/**/*.{scss,sass,css}\" --fix",
    "prettier": "prettier --write 'src/**/*.{js,jsx,ts,tsx,md,json}'",
    "gen:theme-typings": "chakra-cli tokens src/theme/index.ts"
  },
  "pre-commit": [
    "lint"
  ],
  "dependencies": {
    "@chakra-ui/react": "^2.2.1",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "bignumber.js": "^9.1.2",
    "cross-env": "^7.0.3",
    "ethers": "^6.13.2",
    "framer-motion": "^11.5.5",
    "i18next": "^23.15.1",
    "next": "^14.2.12",
    "next-i18next": "^15.3.1",
    "next-images": "^1.8.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^15.0.2",
    "react-toastify": "^10.0.5",
    "sass": "^1.79.1",
    "zustand": "^5.0.0-rc.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.24.7",
    "@chakra-ui/cli": "^2.1.0",
    "@types/mathjs": "^9.4.2",
    "@types/node": "^22.5.5",
    "@types/react": "^18.0.12",
    "@types/react-custom-scrollbars": "^4.0.13",
    "@types/react-dom": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "@typescript-eslint/parser": "^8.6.0",
    "eslint": "^9.10.0",
    "eslint-config-next": "^14.2.12",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "postcss": "^8.4.47",
    "postcss-scss": "^4.0.9",
    "pre-commit": "^1.2.2",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "stylelint": "16.8.2",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-order": "^6.0.4",
    "stylelint-scss": "^6.7.0",
    "typescript": "5.5"
  }
}
